{
  "manifest_version": 3,
  "name": "Find Answers",
  "version": "1.0",
  "description": "Searches the answer for the question selected",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting"
  ],
"icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
    "web_accessible_resources": [
    {
      "resources": ["answers.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
